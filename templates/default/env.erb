PLUGINS="<%= @plugins.join(' ') %>"
DEFAULT_URL=<%= @default_url %>
SECRET_TOKEN=<%= @secret_token %>
FORCE_SSL=<%= @force_ssl ? 1 : 0 %>

<% if @github %>
<%= render "env/github.erb", variables: @github %>
<% end %>
<% if @google %>
<%= render "env/google.erb", variables: @google %>
<% end %>

<% if @docker %>
<%= render "env/docker.erb", variables: @docker %>
<% end %>
<% if @jenkins %>
<%= render "env/jenkins.erb", variables: @jenkins %>
<% end %>
<% if @slack %>
<%= render "env/slack.erb", variables: @slack %>
<% end %>
